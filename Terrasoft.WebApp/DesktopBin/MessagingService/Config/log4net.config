	</logger>
	<logger name="CallwayClient" >
		<level value="TRACE" />
		<appender-ref ref="callwayClientAppender" />
	</logger>
	<logger name="CallwayPacketDispatcher" >
		<level value="TRACE" />
		<appender-ref ref="callwayPacketDispatcherAppender" />
	</logger>
	<logger name="CallwayFrameParser" >
		<level value="TRACE" />
		<appender-ref ref="callwayFrameParserAppender" />
	</logger>
</log4net>