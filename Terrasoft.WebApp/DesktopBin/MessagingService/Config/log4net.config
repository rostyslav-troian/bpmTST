<?xml version="1.0" encoding="utf-8" ?>
<log4net>
	<appender name="asteriskClientAppender" type="log4net.Appender.RollingFileAppender">
		<filter type="log4net.Filter.LevelMatchFilter">
			<levelToMatch value="ALL" />
		</filter>
		<File value="Logs\asterisk\asteriskClient\" />
		<appendToFile value="true" />
		<rollingStyle value="Date" />
		<datePattern value="yyyyMMdd-HH'.log'" />
		<staticLogFileName value="false" />
		<layout type="log4net.Layout.PatternLayout">
			<conversionPattern value="%date %-5level %message%newline" />
		</layout>
	</appender>
	<appender name="asteriskClientNetworkIncomingTraceAppender" type="log4net.Appender.RollingFileAppender">
		<filter type="log4net.Filter.LevelMatchFilter">
